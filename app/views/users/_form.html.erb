<%= form_for user do |f| %>
  <div>
    <%= f.label 'First Name' %>
    <%= f.text_field :first_name %>
  </div>

  <div>
    <%= f.label 'Last Name' %>
    <%= f.text_field :last_name %>
  </div>

  <div>
    <%= f.label 'Email' %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label 'Phone' %>
    <%= f.text_field :phone %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
